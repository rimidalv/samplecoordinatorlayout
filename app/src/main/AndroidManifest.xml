<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anews.samplecoordinatorlayout" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleActivity"
            android:label="@string/title_activity_simple" >
        </activity>
        <activity
            android:name=".Simple3XActivity"
            android:label="@string/title_activity_simple2_x" >
        </activity>
        <activity
            android:name=".CollapsingExitUntilActivity"
            android:label="@string/title_activity_collapsing" >
        </activity>
        <activity
            android:name=".SimpleEnterAlwaysActivity"
            android:label="@string/title_activity_simple_enter_always" >
        </activity>
        <activity
            android:name=".SimpleEnterAlwaysCollapsedActivity"
            android:label="@string/title_activity_simple_enter_always_collapsed" >
        </activity>
        <activity
            android:name=".CollapsingEnterAlwaysActivity"
            android:label="@string/title_activity_collapsing_enter_always" >
        </activity>
        <activity
            android:name=".CollapsingEnterAlwaysCollapsedActivity"
            android:label="@string/title_activity_collapsing_enter_always_collapsed" >
        </activity>
        <activity
            android:name=".CollapsingEnterAlwaysCollapsedEnterAlwaysActivity"
            android:label="@string/title_activity_collapsed_enter_always_collapsed_enter_always" >
        </activity>
        <activity
            android:name=".CollapsingEnterAlwaysExitActivity"
            android:label="@string/title_activity_collapsing_enter_always_exit" >
        </activity>
        <activity
            android:name=".CollapsingEnterAlwaysCollapsedExitActivity"
            android:label="@string/title_activity_collapsing_enter_always_collapsed_exit" >
        </activity>
        <activity
            android:name=".FABActivity"
            android:label="@string/title_activity_fab" >
        </activity>
        <activity
            android:name=".TabFABActivity"
            android:label="@string/title_activity_tab_fab" >
        </activity>
    </application>

</manifest>
